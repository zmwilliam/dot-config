set -g status-bg "#504945"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g status-right-length "100"
set -gq status-utf8 "on"
set -g status-left "#[fg=#7c6f64,bg=#282828,bold] ðŸ’»  #(whoami) î‚± #S #[fg=#282828,bg=#504945,nobold,nounderscore,noitalics]î‚°"
set -g status-right "#[fg=#504945,bg=#504945,nobold,nounderscore,noitalics]î‚²#[fg=#a89984,bg=#504945,bold] #{prefix_highlight} #[fg=#282828,bg=#504945,nobold,nounderscore,noitalics]î‚²#[fg=#7c6f64,bg=#282828,bold] #{cpu_icon}#{cpu_percentage} î‚³ #{online_status} î‚³ #{battery_icon} #{battery_percentage} î‚³ %R "
setw -g window-status-format '#[fg=#504945,bg=#504945,nobold,nounderscore,noitalics]î‚°#[default] #I î‚± #(pwd="#{pane_current_path}"; echo ${pwd####*/}) #[fg=#504945,bg=#504945,nobold,nounderscore,noitalics]î‚°'
